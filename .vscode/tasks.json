// .vscode/tasks.json

{
	"version": "2.0.0",
	"tasks": [
	  {
		"label": "npm: start",
		"type": "npm",
		"script": "start",
		"isBackground": true,
		"presentation": {
		  "echo": true,
		  "reveal": "always",
		  "focus": false,
		  "panel": "shared",
		  "showReuseMessage": true,
		  "clear": false
		},
		"problemMatcher": {
		  "owner": "custom",
		  "pattern": {
			"regexp": ".",
			"file": 1,
			"location": 2,
			"message": 3
		  },
		  "background": {
			"activeOnStart": true,
			"beginsPattern": ".",
			"endsPattern": "Compiled successfully."
		  }
		}
	  }
	]
  }
  